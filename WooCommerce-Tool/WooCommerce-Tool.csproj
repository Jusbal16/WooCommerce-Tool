    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>WooCommerce_Tool</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\delete.png" />
